{
  "name": "Pizzawala",
  "version": "1.0.0",
  "description": "This is our pizza delivery app",
  "main": "index.js",
  "author": "Nidhi",
  "scripts": {
    "serve": "node server.js",
    "dev": "nodemon server.js",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --config=node_modules/laravel-mix/setup/webpack.config.js --watch --stats-children",
    "watch": "npm run development -- --watch",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "license": "MIT",
  "dependencies": {
    "cross-env": "^7.0.3",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-ejs-layouts": "^2.5.1",
    "nodemon": "^3.1.7",
    "semver": "^7.6.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "laravel-mix": "^6.0.49",
    "resolve-url-loader": "5.0.0",
    "sass": "^1.77.8",
    "sass-loader": "12.1.0"
  }
}
